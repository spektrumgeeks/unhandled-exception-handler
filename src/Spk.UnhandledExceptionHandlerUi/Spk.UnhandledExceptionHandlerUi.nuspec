<?xml version="1.0"?>
<package >
    <metadata>
        <id>Spk.UnhandledExceptionHandlerUi</id>
        <version>1.3.0</version>
        <title>An exception catcher for .Net MVC 4 and 5. Unhandled Exception Handler UI</title>
        <authors>spektrummedia</authors>
        <owners>Spektrum Media</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <iconUrl>https://avatars1.githubusercontent.com/u/13052913</iconUrl>
        <description>Manages unhandled exceptions thrown in an application by adding log entries and sending emails.</description>
        <releaseNotes></releaseNotes>
        <copyright>Copyright Spektrum Media 2018</copyright>
        <tags>unhandled exception error</tags>
        <dependencies>
            <dependency id="Spk.UnhandledExceptionHandlerCore" version="1.3.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="content\*.*" target="content" />
        <file src="content\App_Start\*.*" target="content\App_Start" />
        <file src="content\Views\**\*.cshtml" target="content\Views" />
    </files>
</package>