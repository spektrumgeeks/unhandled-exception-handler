<?xml version="1.0"?>
<package >
    <metadata>
        <id>Spk.Infrastructure.UnhandledExceptionHandlerUi</id>
        <version>1.2.4</version>
        <title>Unhandled Exception Handler UI</title>
        <authors>Spektrum Media</authors>
        <owners>Spektrum Media</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Manages unhandled exceptions thrown in an application by adding log entries and sending emails.</description>
        <releaseNotes></releaseNotes>
        <copyright>Copyright Spektrum Media 2018</copyright>
        <tags>unhandled exception error</tags>
        <dependencies>
            <dependency id="Spk.UnhandledExceptionHandlerCore" version="1.2.4" />
        </dependencies>
    </metadata>
    <files>
        <file src="content\*.*" target="content" />
        <file src="content\App_Start\*.*" target="content\App_Start" />
        <file src="content\Views\**\*.cshtml" target="content\Views" />
    </files>
</package>